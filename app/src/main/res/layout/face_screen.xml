<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="130dp"
        android:layout_height="117dp"
        android:layout_marginStart="388dp"
        android:layout_marginTop="28dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:srcCompat="@drawable/group_5" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="127dp"
        android:layout_height="115dp"
        android:layout_marginEnd="341dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView11"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/group_3"
        tools:layout_editor_absoluteY="148dp" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="125dp"
        android:layout_height="123dp"
        android:layout_marginStart="124dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView11"
        app:srcCompat="@drawable/group_4"
        tools:layout_editor_absoluteY="144dp" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="774dp"
        android:layout_height="107dp"
        app:srcCompat="@drawable/intersect"
        tools:layout_editor_absoluteX="-7dp"
        tools:layout_editor_absoluteY="310dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="356dp"
        android:layout_marginTop="16dp"
        android:text="PLEASE TOUCH ONE!"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>